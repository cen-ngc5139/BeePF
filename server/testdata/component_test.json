{
    "id": 1,
    "name": "测试组件",
    "programs": [
        {
            "id": 1,
            "name": "测试程序1",
            "description": "这是一个测试程序，用于监控系统调用",
            "spec": {
                "name": "test_program_1",
                "type": 1,
                "attach_type": 2,
                "attach_to": "sys_open",
                "section_name": "kprobe/sys_open",
                "flags": 0,
                "license": "GPL",
                "kernel_version": 0
            },
            "properties": {
                "debug_verbose": true,
                "print_header": true,
                "enable_multiple_export_types": false,
                "perf_buffer_pages": 64,
                "perf_buffer_time_ms": 10,
                "poll_timeout_ms": 100
            }
        },
        {
            "id": 2,
            "name": "测试程序2",
            "description": "这是一个测试程序，用于跟踪网络数据包",
            "spec": {
                "name": "test_program_2",
                "type": 3,
                "attach_type": 4,
                "attach_to": "eth0",
                "section_name": "xdp/eth0",
                "flags": 0,
                "license": "GPL",
                "kernel_version": 0
            },
            "properties": {
                "debug_verbose": true,
                "print_header": true,
                "enable_multiple_export_types": true,
                "perf_buffer_pages": 128,
                "perf_buffer_time_ms": 20,
                "poll_timeout_ms": 200
            }
        }
    ],
    "maps": [
        {
            "id": 1,
            "name": "测试Map1",
            "description": "这是一个测试Map，用于存储系统调用统计信息",
            "spec": {
                "name": "test_map_1",
                "type": 1,
                "key_size": 4,
                "value_size": 8,
                "max_entries": 1024,
                "flags": 0,
                "pinning": 1
            },
            "properties": {
                "sample": {
                    "interval": 1000,
                    "type": "histogram",
                    "unit": "ms",
                    "clear_map": true
                },
                "export_config": "default",
                "interpreter": {
                    "type": "default_struct"
                }
            }
        },
        {
            "id": 2,
            "name": "测试Map2",
            "description": "这是一个测试Map，用于存储网络数据包信息",
            "spec": {
                "name": "test_map_2",
                "type": 2,
                "key_size": 8,
                "value_size": 16,
                "max_entries": 2048,
                "flags": 0,
                "pinning": 2
            },
            "properties": {
                "sample": {
                    "interval": 2000,
                    "type": "counter",
                    "unit": "packets",
                    "clear_map": false
                },
                "export_config": "json",
                "interpreter": {
                    "type": "stack_trace",
                    "stack_trace": {
                        "field_map": {
                            "pid": "process_id",
                            "comm": "command",
                            "kstack_sz": "kernel_stack_size",
                            "ustack_sz": "user_stack_size"
                        },
                        "with_symbols": true
                    }
                }
            }
        }
    ]
}